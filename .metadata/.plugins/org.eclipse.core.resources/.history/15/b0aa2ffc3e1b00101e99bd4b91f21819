package chapter05;

public class FuncTest_07 {
	
	
	public static void swap(int x, int y) {
		
		int temp=x;
		System.out.println(temp);
		
	}//
	
	public static void main(String[] args) {
		
		int a=10;
		int b=20;
		System.out.println("a: "+a+" b: "+b);
		System.out.println("--------------------");
		
		swap(b,a);
		System.out.println("a: "+a+" b: "+b);
		System.out.println("--------------------");
		
		
		
		
		
		
	}

}
