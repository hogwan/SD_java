package chapter08;

public class SportsCar extends Car {

	@Override
	public void speedUp() {
		speed += 10;
		System.out.println("speed: " + speed);
	}

	@Override
	public void stop() {
		System.out.println("스포츠카가 멈춥니다.");
		speed = 0;
	}

	public static void main(String[] args)
	{
		SportsCar ac = new SportsCar();
		ac.speedUp();
		ac.stop();
	}
}
