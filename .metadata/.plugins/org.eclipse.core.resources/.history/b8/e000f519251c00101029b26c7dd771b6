package chapter06;

public class StudentInfo {
	// 멤버변수=필드
	private int studentID;
	private String studentName;
	private int grade;
	private String address;

	// 생성자
	public StudentInfo() {
	}

	// 생성자 오버로딩
	public StudentInfo(int id, String name, int grade, String address) {
		this.studentID = id;
		this.studentName = name;
		this.grade = grade;
		this.address = address;
	}
}
