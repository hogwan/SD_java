package chapter06;

public class Vending {
	private Can[] can = new Can[5];
	private int money;
	
	//생성자
	
	//메소드
	public void init() {
		can[0] = new Can("환타", 1000);
		can[0] = new Can("사이다", 1000);
		can[0] = new Can("오렌지쥬스", 1000);
		can[0] = new Can("바나나우유", 1000);
		can[0] = new Can("콜라", 1000);
	}
}
