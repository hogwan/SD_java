package chapter04;

public class ArrayTest_06 {

	public static void main(String[] args) {
		
		String str[]= {"홍길동","김유신","장길산","이순신"};
		String result="";
		
		for(int i=0;i<str.length;i++) {
			System.out.print(str[i]);
			result+= str[i]+"\t";
		}
		System.out.println();
		System.out.println(result);
	}
}
