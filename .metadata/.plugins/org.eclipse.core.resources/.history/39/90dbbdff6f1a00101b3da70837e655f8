package chapter04;

public class TwoDimension_09 {

	public static void main(String[] args) {
		// 이차원 배열
		int arr[][]=new int[2][3]; //2행 3열 arr.length=줄 arr[0].length=칸
		int n=0;
		
		for(int i=0;i<arr.length;i++) {
			for(int j=0;j<arr[i].length;j++) {
				n++;
				System.out.print(arr[i][j]=n);
			}
			System.out.println();
		}//f
		
		
	}//m

}//c
