package chapter04;

public class ArrayTest_05 {

	public static void main(String[] args) {
		
		double[] data=new double[5];
		int size=0;
				
		//대입연산
		data[0]=10.0;
		size++; //size=size+1
		data[1]=20.0;
		size++; //1
		data[2]=30.0;
		size++; //2
		
		double total=1.0;
		
		System.out.println(size);
		
	}

}
