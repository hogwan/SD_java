package chapter08;

public class Washer extends Appliance {
	public Washer(String brand) {
		super(brand);
	}

	@Override
	void turnOn() {
		// TODO Auto-generated method stub
		super.turnOn();
		System.out.println("세탁기를 작동시킵니다.");
	}

	@Override
	void turnOff() {
		// TODO Auto-generated method stub
		super.turnOff();
	}
	
	
}
