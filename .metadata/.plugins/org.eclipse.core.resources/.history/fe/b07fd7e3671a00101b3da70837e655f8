package chapter04;

public class ArrayTest_06 {

	public static void main(String[] args) {
		
		String str[]= {"홍길동","김유신","장길산","이순신"};
		String result="";
		
		str[0]="홍길동";
		str[1]="김유신";
		str[2]="장길산";
		str[3]="이순신";
		
		for(int i=0;i<str.length;i++) {
			System.out.println(str[i]);
			result+=str[i];
		}
		System.out.println(result);
	}
}
