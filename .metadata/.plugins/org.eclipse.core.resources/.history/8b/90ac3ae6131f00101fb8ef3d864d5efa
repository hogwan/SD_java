package chapter08;

public class StraawBerry {
	private String color;    // 분류 
	private String price;  // 계절 과일
	
	public void set1(String sort, String season)
	{
		this.sort = sort;
		this.season = season;
	}
	
	public void Disp1() {
		System.out.println("분류 : " + sort);
		System.out.println("계절 : " + season);
	}
}
