package chapter05;
	
public class MethodTest_13 {
	
	public static void main(String[] args) {
		
		int sum=add(5,7);
		double sumDouble=add(3.5,7.2);
		
		
	}
	
	//정수의 합
	static int add(int a,int b) {
		int add=a+b;
		
		
		return=add
	}
	
	
	
	//실수의 합
	static double add(int a,int b) {
		double add=a+b;
		
		return=add;
	}
	
}	
