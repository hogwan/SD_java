package chapter06;

import java.util.Scanner;

public class CanMain_09 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int money = 0;
		
		//음료 준비
		Vending vend = new Vending();
		vend.setMoney(money);
		vend.init();
		vend.showCans(1000);
		vend.outCan("환타");
		
		scan.close();
	}
}
